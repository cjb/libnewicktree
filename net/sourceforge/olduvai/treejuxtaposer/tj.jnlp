      <j2se href="http://java.sun.com/products/autodl/j2se" version="1.5 1.4+ 1.4*" initial-heap-size="128M" max-heap-size="1700M"/>
        <jar href="tj.jar" main="true"/>
        <jar href="lib/jogl.jar"/>
      <nativelib href = "lib/libjogl.so"/>
